<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8" />
<meta name="Keywords" content="{$keywords}" />
<meta name="Description" content="{$description}" />
<!-- TemplateBeginEditable name="doctitle" -->
<title>{$page_title}</title>
<!-- TemplateEndEditable -->
<!-- TemplateBeginEditable name="head" -->
<!-- TemplateEndEditable -->
<link rel="shortcut icon" href="favicon.ico" />
<link href="{$ecs_css_path}" rel="stylesheet" type="text/css" />
</head>
<body>
<!-- #BeginLibraryItem "/library/page_header.lbi" --><!-- #EndLibraryItem -->
{* 包含脚本文件 *}
{insert_scripts files='jquery.slides.js,common.js,index.js'}
<div id="J-slider" class="ms-slider">
  <!--{foreach from=$page_data.banner item=banner name=banner}-->
	<div class="item" style="background-image:url({$banner.img})">
	  <a href="{$banner.href}" target="_blank"></a>
	</div>
  <!--{/foreach}-->
</div>

<!--floors-->
<!--{foreach from=$page_data.floors item=floor name=floor}-->

<!--{if $floor.enabled}-->
<div class="ms-floor ms-floor-{$smarty.foreach.floor.index}">
  <div class="ms-mod clearfix">
    <h3 class="clearfix">
      <ul class='q-link clearfix'>
        <!--{foreach from=$floor._tags item=tag name=tag}-->
         <li><a href="{$tag.url}" target="_blank">{$tag.label}</a></li>
        <!--{/foreach}-->
      </ul>
    </h3>
    <div class="floor-cate">
      <div class="slogan">
        <img src="{$floor.slogan.img}"/>
	      <a href="{$floor.slogan.href}" target="_blank"></a>
      </div>
      <div class="menu clearfix">
        <ul class='q-link2 clearfix'>
          <!--{foreach from=$floor._tags item=tag name=tag}-->
           <li><a href="{$tag.url}" target="_blank">{$tag.label}</a></li>
          <!--{/foreach}-->
         </ul>
      </div>
      <ul>
    </div>
    <div class="goods clearfix">
      <div class="column column-1">

        <p>
	 <a class="first-x first-y" href="{$floor.items[0].href}" target="_blank">
	   <img src="{$floor.items[0].img}" width="200" height="140"/>
	   <span class="deco">
	     <em class="flash-buy-price">闪购价：¥<span class="mui-price-font">{$floor.items[0].price}</span></em>
	   </span>
	 </a>
	</p>

	<p>
	 <a class="first-x" href="{$floor.items[1].href}" target="_blank">
	  <img src="{$floor.items[1].img}" width="200" height="140"/>
	   <span class="deco">
	     <em class="flash-buy-price">闪购价：¥<span class="mui-price-font">{$floor.items[1].price}</span></em>
	   </span>
	 </a>
	</p>

        <p>
	 <a class="first-x" href="{$floor.items[2].href}" target="_blank">
	  <img src="{$floor.items[2].img}" width="200" height="140"/>
	   <span class="deco">
	     <em class="flash-buy-price">闪购价：¥<span class="mui-price-font">{$floor.items[2].price}</span></em>
	   </span>
	 </a>
	</p>

      </div>
      <div class="column column-2">
        <p>
	  <a class="first-y" href="{$floor.items[3].href}" target="_blank">
	    <img src="{$floor.items[3].img}" width="399" height="422"/>
	   <span class="deco">
	     <em class="flash-buy-price">闪购价：¥<span class="mui-price-font">{$floor.items[3].price}</span></em>
	   </span>
	  </a>
	</p>
      </div>
      <div class="column column-3">
         <p class="first">
	   <a class="first-y" href="{$floor.items[4].href}" target="_blank">
	     <img src="{$floor.items[4].img}" width="199" height="281"/>
	     <span class="deco">
	       <em class="flash-buy-price">闪购价：¥<span class="mui-price-font">{$floor.items[4].price}</span></em>
	     </span>
	   </a>
	  </p>
	 <p class="last">
	   <a href="{$floor.items[5].href}" target="_blank">
	     <img src="{$floor.items[5].img}" width="199" height="140"/>
	     <span class="deco">
	       <em class="flash-buy-price">闪购价：¥<span class="mui-price-font">{$floor.items[5].price}</span></em>
	     </span>
	   </a>
	 </p>
      </div>
    </div>
  </div>
</div>
<!-- {/if} -->
<!--{/foreach}-->


<!-- 最新 -->
<div class="ms-new">
 <div class="ms-mod">
  <h3>女装最新上市</h3>
  <ul class="clearfix">
    <!--{foreach from=$page_data.latest item=goods name=goods}-->
    <li {if $smarty.foreach.goods.index eq 0} class='first'{/if}>
      <a href="{$goods.href}" target="_blank">
        <span class="img">
	   <img data-src="{$goods.thumb}" src="{$goods.img}" width="190" height="290"/>
	</span>
	<!--{if $goods.price}-->
	<span class="summary">
	   <em class="flash-buy-price">闪购价：¥{$goods.price}</em>
	</span>
	<!--{/if}-->
      </a>
    </li>
    <!--{/foreach}-->
  </ul>
 </div>
</div>

<!-- sys -->
<div class="ms-sys">
  <div class="ms-mod clearfix">
    <div class="ms-intro sys-item">
      <h3>新手教程<span>Novice Tutorials</span></h3>
      <div class="bd">
       <ul>
         <!--{foreach from=$articles_newbie item=article name=article}-->
         <li {if $smarty.foreach.article.index eq 0} class='first'{/if}>
           <a href="{$article.url}" target="_blank">{$article.title}</a>
         </li>
         <!--{/foreach}-->
       </ul>
      </div>
    </div>
    <div class="ms-lect sys-item">
      <h3>时尚讲堂<span>Fasion Lecture</span></h3>
      <div class="bd">
       <ul>
         <!--{foreach from=$articles_lecture item=article name=article}-->
         <li {if $smarty.foreach.article.index eq 0} class='first'{/if}>
           <a href="{$article.url}" target="_blank">{$article.title}</a>
         </li>
         <!--{/foreach}-->
         
       </ul>
      </div>
    </div>
    <div class="ms-notice sys-item">
      <h3>网站公告<span>Site Notice</span></h3>
      <div class="bd">
	 <ul>
          <!--{foreach from=$articles_notice item=article name=article}-->
          <li {if $smarty.foreach.article.index eq 0} class='first'{/if}>
            <a href="{$article.url}" target="_blank">{$article.title}</a>
          </li>
          <!--{/foreach}-->
        </ul>
      </div>
    </div>
  </div>
</div>


<div class="ms-shop-tip ms-mod">
  <img src="../images/vantage.jpg" width="1000" height="124"/>
</div>
<div class="ms-ensure">
  <div class="ms-mod">
    <div class="pay-image"></div>
  </div>
</div>
<script>
$(function(){
  $("#J-slider").slidesjs({
    navigation:{
	    active:false
	  },
    play:{
      auto: true
    },
	  width: 1600,
	  height: 400
  });
});
</script>
<!-- #BeginLibraryItem "/library/page_footer.lbi" --><!-- #EndLibraryItem -->
</body>
</html>
